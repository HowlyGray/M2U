{
  "rules": {
      ".read": true,
      ".write": true,
      "$peerId": {
        ".read": true,
        ".write": true,
        ".validate": "newData.hasChildren(['name', 'timestamp', 'online', 'peerId'])",
        ".indexOn": ["timestamp", "online"]
      }
    },
    ".read": false,
    ".write": false
  }
}
